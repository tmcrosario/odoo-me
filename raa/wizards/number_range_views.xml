<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- TREE VIEW -->
    <record id="range_view_tree"
            model="ir.ui.view">
        <field name="name">raa.range.view.tree</field>
        <field name="model">raa.range</field>
        <field name="arch"
               type="xml">
            <tree string="Document Range"
                  editable="bottom">
                <field name="number_from" />
                <field name="number_to" />
            </tree>
        </field>
    </record>

</odoo>
