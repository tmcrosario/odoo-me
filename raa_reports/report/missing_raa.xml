<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="missing_raa"
            model="ir.actions.report">
        <field name="name">Missing Administrative Acts Report</field>
        <field name="model">raa.entry.wizard</field>
        <field name="report_name">raa.missing_raa</field>
        <field name="report_type">py3o</field>
        <field name="py3o_filetype">pdf</field>
        <field name="py3o_server_id"
               ref="tmc_data.local_py3o_server" />
        <field name="py3o_is_local_fusion"
               eval="1" />
        <field name="module">raa</field>
        <field name="py3o_template_fallback">report/missing_raa.odt</field>
    </record>

</odoo>